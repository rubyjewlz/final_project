<div class="teal">
	<%= form_for @track do |f| %>
	<% if @track.errors.any? %>
		<div id="error_explanation">
			<h3>
				<%= pluralize(@track.errors.count, "error") %>
				prohibited
				this track from being saved:
			</h3>
			<ul>
				<% @track.errors.full_messages.each do |msg| %>
					<li>
						<%= msg %>
					</li>
				<% end %>
			</ul>
		</div>
	<% end %>
		<!-- <p>
			<%= f.label :title %>
			<br>
			<%= f.text_field :title %>
		</p> -->
		<p>
			<%= f.label :url %>
			<br>
			<%= f.text_area :text %>
		</p>
		<p>
			<%= f.submit %>
		</p>
<% end %>
</div>
<br>
